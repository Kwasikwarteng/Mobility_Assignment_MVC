<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Mobility_Assignment_MVC</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/Mobility_Assignment_MVC.styles.css" asp-append-version="true" />
</head>
<body>
    <header>
        <nav class="nav flex-column">
            <a class="nav-link" asp-area="" asp-controller="LinkEntity" asp-action="Index">Link Entity</a>
           @*  <a class="nav-link" asp-area="" asp-controller="LinkEntity" asp-action="WebService">Swagger UI</a> *@
            <a class="nav-link" asp-area="" asp-controller="LinkEntity" asp-action="WebServiceUI">Web Service</a>
            @* <a class="nav-link" href="@Url.Action("Index", "WebServiceHome", new { area = "" })">Web Service</a> *@
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2024 - Mobility_Assignment_MVC - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
